{
	"extends": ["plugin:@nrwl/nx/react", "plugin:jsx-a11y/strict"],
	"root": true,
	"ignorePatterns": ["!**/*"],
	"plugins": ["@nrwl/nx"],
	"parser": "@typescript-eslint/parser",
	"rules": {},
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"extends": ["plugin:@nrwl/nx/typescript"],
			"rules": {}
		},
		{
			"files": ["*.js", "*.jsx"],
			"extends": ["plugin:@nrwl/nx/javascript"],
			"rules": {}
		}
	]
}
