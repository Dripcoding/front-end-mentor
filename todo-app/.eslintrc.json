{
  "extends": ["plugin:@nrwl/nx/react"],
  "root": true,
  "ignorePatterns": ["!**/*"],
  "plugins": ["@nrwl/nx"],
  "parser": "@typescript-eslint/parser",
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "extends": ["plugin:@nrwl/nx/typescript"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": ["plugin:@nrwl/nx/javascript"],
      "rules": {}
    }
  ]
}
